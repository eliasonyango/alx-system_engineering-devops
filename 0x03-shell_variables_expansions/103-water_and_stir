#!/bin/bash
# Convert the values in WATER and STIR from base water and base stir to base 10
water=$(echo "$WATER" | tr 'water' '01234' | tr -d '\n')
stir=$(echo "$STIR" | tr 'stir' '56789' | tr -d '\n')
# Add the converted values
result=$((water + stir))
# Convert the result back to base bestchol
bestchol=$(echo "obase=bestchol; $result" | bc)
# Print the result
echo "$bestchol"l
